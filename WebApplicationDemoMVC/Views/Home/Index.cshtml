@Html.ActionLink("Bam vao day", "MyAction", "Home", new { name = "quan", id = 2022 })
<div class="row">


    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null && Model.Count > 0)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                        <td><a href="/trang-chi-tiet-@item.Name/@item.Id">Views Detail </a></td>
                    </tr>

                }
            }
        </tbody>
    </table>
</div>