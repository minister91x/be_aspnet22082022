@{
    ViewBag.Title = "Home Page";
}
@*@model List<WebApplicationDemoMVC.Models.Student>*@
@model List<WebApplicationDemoMVC.Models.Student>
<a href="@Html.Action("MyAction", "Home", new { name = "quan", id = 2022 })"> Bấm vào đây </a>
@Html.ActionLink("Bam vao day", "MyAction", "Home", new { name = "quan", id = 2022 })
<div class="row">


    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @if (Model != null && Model.Count > 0)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@item.Name</td>
                    </tr>

                }
            }
        </tbody>
    </table>
</div>